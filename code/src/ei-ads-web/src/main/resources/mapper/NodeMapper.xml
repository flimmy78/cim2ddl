<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.znd.ei.ads.web.mapper.NodeMapper">
    <!--通过name获取Node对象-->
    <select id="getByName" parameterType="String" resultType="NodeInfo">
        SELECT * from ads_node where name = #{name}
    </select>

    <!--通过地址Node对象-->
    <select id="getByUrl" parameterType="String" resultType="NodeInfo">
        SELECT * from ads_node where url = #{url}
    </select>

    <select id="all" resultType="NodeInfo">
        select * from ads_node
    </select>

    <!--通过username获取user对象-->
    <insert id="save" useGeneratedKeys="true"   keyProperty="id">
        insert into ads_node(name, url) VALUES (#{name},#{url})
    </insert>
    
</mapper>
