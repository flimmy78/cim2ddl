						<xs:annotation>
							<xs:documentation>Non-linear shunt compensator phase owning this point.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="NonlinearShuntCompensatorPhasePoint" type="NonlinearShuntCompensatorPhasePoint"/>
	<xs:complexType name="NonlinearShuntCompensatorPhasePoint">
		<xs:annotation>
			<xs:documentation>A per phase non linear shunt compensator bank or section admittance value.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="b" type="Susceptance" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Positive sequence shunt (charging) susceptance per section</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="g" type="Conductance" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Positive sequence shunt (charging) conductance per section</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="sectionNumber" type="Integer" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The number of the section.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="PerLengthLineParameter" type="PerLengthLineParameter"/>
	<xs:complexType name="PerLengthLineParameter">
		<xs:annotation>
			<xs:documentation>Common type for per-length electrical catalogues describing line parameters.</xs:documentation>
		</xs:annotation>
